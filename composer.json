{
  "require-dev": {
    "pestphp/pest": "^3.8",
    "phpunit/phpunit": "^11.5",
    "laravel/pint": "^1.22",
    "phpstan/phpstan": "^2.1"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "autoload": {
    "psr-4": {
      "Router\\": "Router/"
    }
  },
  "autoload-dev": {
